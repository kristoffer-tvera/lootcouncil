@page
@model Lootcouncil.Pages.SecureModel
@{
}

<h2>Claims</h2>

<dl>
    @foreach (var claim in User.Claims)
    {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>
    }
</dl>


<dl>
    @foreach (var character in Model.Characters)
    {
        <dt>@(character.Realm.Name + " " + character.Name)</dt>
        <dd>@(character.Level)</dd>
    }
</dl>